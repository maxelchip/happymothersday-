{
    "manifest_version": 3,
    "name": "Happy_Mother's_Day!",
    "version": "1.0",
    "permissions": [
      "tabs"
    ],
    "action": {
      "default_popup": "popup.html"
    },
    "content_scripts": [
        {
          "matches": ["<all_urls>"],
          "js": ["script.js"]
        }
      ],
    "web_accessible_resources": [
        {
            "resources": [
              "popup.html"
            ],
            "matches": [
              "http://*/*",
              "https://*/*"
            ]
        }
    ]
  }
  