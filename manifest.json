{
    "manifest_version": 2,
    "name": "Happy_Mother's_Day!",
    "version": "1.0",
    "permissions": [
      "tabs",
      "activeTab"
    ],
    "browser_action": {
      "default_popup": "popup.html"
    },
    "content_scripts": [
        {
          "matches": ["<all_urls>"],
          "js": ["script.js"]
        }
      ],
      "web_accessible_resources": [
        "popup.html"
      ]
  }
  